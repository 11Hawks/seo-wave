{
  "name": "seo-analytics-platform",
  "version": "1.0.0",
  "description": "Comprehensive SEO analytics and marketing platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "node --max-old-space-size=4096 ./node_modules/.bin/next lint",
    "lint:fix": "node --max-old-space-size=4096 ./node_modules/.bin/next lint --fix",
    "type-check": "node --max-old-space-size=4096 ./node_modules/.bin/tsc --noEmit",
    "type-check:watch": "node --max-old-space-size=4096 ./node_modules/.bin/tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "clean": "rm -rf .next build dist out coverage",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "memory:check": "node --expose-gc --max-old-space-size=4096 -e 'console.log(process.memoryUsage())'",
    "deps:check": "npm list --depth=0",
    "deps:outdated": "npm outdated",
    "security:audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.8.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^3.2.0",
    "framer-motion": "^10.18.0",
    "google-auth-library": "^9.5.0",
    "googleapis": "^131.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.312.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.10.4",
    "stripe": "^14.12.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.25.76",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.3.2",
